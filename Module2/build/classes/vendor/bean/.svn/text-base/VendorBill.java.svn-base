package vendor.bean;

import java.sql.Date;

public class VendorBill {
	/**
	 * @author swedha
	 * @class name VendorBill
	 * @createDate 23july2015
	 */
	private String poNo;
	private String vendorNo;
	private String billNo;
	private String typeOfDelivery;
	private String status;
	private String readyForShipment;
	private String directToStore;
	private Date dateOFDelivery;
	private String orderBy;
	public VendorBill(String poNo, String vendorNo, String billNo,
			String typeOfDelivery, String status, String readyForShipment,
			String directToStore, Date dateOFDelivery, String orderBy) {
		super();
		this.poNo = poNo;
		this.vendorNo = vendorNo;
		this.billNo = billNo;
		this.typeOfDelivery = typeOfDelivery;
		this.status = status;
		this.readyForShipment = readyForShipment;
		this.directToStore = directToStore;
		this.dateOFDelivery = dateOFDelivery;
		this.orderBy = orderBy;
	}
	public String getPoNo() {
		return poNo;
	}
	public void setPoNo(String poNo) {
		this.poNo = poNo;
	}
	public String getVendorNo() {
		return vendorNo;
	}
	public void setVendorNo(String vendorNo) {
		this.vendorNo = vendorNo;
	}
	public String getBillNo() {
		return billNo;
	}
	public void setBillNo(String billNo) {
		this.billNo = billNo;
	}
	public String getTypeOfDelivery() {
		return typeOfDelivery;
	}
	public void setTypeOfDelivery(String typeOfDelivery) {
		this.typeOfDelivery = typeOfDelivery;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public String getReadyForShipment() {
		return readyForShipment;
	}
	public void setReadyForShipment(String readyForShipment) {
		this.readyForShipment = readyForShipment;
	}
	public String getDirectToStore() {
		return directToStore;
	}
	public void setDirectToStore(String directToStore) {
		this.directToStore = directToStore;
	}
	public Date getDateOFDelivery() {
		return dateOFDelivery;
	}
	public void setDateOFDelivery(Date dateOFDelivery) {
		this.dateOFDelivery = dateOFDelivery;
	}
	public String getOrderBy() {
		return orderBy;
	}
	public void setOrderBy(String orderBy) {
		this.orderBy = orderBy;
	}
	
}
